@model IEnumerable<InternetShop.Models.Good>
@{
    ViewBag.Title = "Home Page";
    int i = 0;
    int len = 0;
}
<style>
    #test {
        position: relative;
        float:left;
        width:200px;
        height:300px;
        margin-left:20px;
    }
    
</style>
<br />
@foreach (var item in Model)
{
    <a href="@Url.Action("Select", "Home", new { id = item.Id })">
        <div class="well" id="test">
            <img src="@item.Image">
            <h3>
                <strong>@item.Name</strong>
                <span class="pull-right label label-primary">@item.Price.ToString("# руб")</span>
            </h3>
            <span class="lead">@item.Description</span>
        </div>
    </a>
}
